#include <iostream>
#include <iomanip>
#include <set>
#include <cctype>
#include <algorithm>
using namespace std;


int main()
{
    string n;
    cin >>n;
    char tab[]={'h','e','l','l','o'};

    int len=n.length();
    int j=0;
    for (int i=0;i<len;i++){
        if (n[i]==tab[j]){
            j++;
        }
    }
    if (j==5){
        cout<<"YES"<<endl;
    }
    else {
        cout <<"NO"<<endl;
    }




    return 0;
}
