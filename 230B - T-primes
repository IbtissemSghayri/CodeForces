#include <iostream>
#include <iomanip>
#include <set>
#include <cctype>
#include <algorithm>
#include <cmath>
using namespace std;


int main()
{
    int n;
    cin >>n;
    long long a;

    for (int i=0;i<n;i++){
        cin >> a;
        long long b=sqrt(a);
        if (b*b==a && a>3){
            int j=2;
            bool test=true;
            int k=sqrt(b);
            while (j<k+1){
                if (b%j==0){
                    test=false;
                    break;
                }
                else {
                    j++;
                }
            }
            if (test==true){
                cout<<"YES"<<endl;
            }
            else {
                cout<<"NO"<<endl;
            }
        }
        else {
            cout <<"NO"<<endl;
        }
    }






    return 0;
}
